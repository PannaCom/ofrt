@model WebOfficeRental.Models.LstMenu
@foreach (var item in Model.LstMenus)
{
    if (item != null)
    {
        <option value="@item.MenuId" class="@if(item.LstMenus.Count > 0) {@Html.DisplayName("font-w-bold")} else {}">@item.MenuName</option>
        if (item.LstMenus.Count > 0)
        {
            @Html.Partial("_childmenuPartial", item)
        }
    }
}
