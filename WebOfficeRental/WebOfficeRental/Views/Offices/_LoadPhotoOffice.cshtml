@model List<WebOfficeRental.Models.office_photos>
@if (Model != null)
{
    if (Model.Count > 0)
    {
        <div class="table-responsive">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Hình ảnh</th>
                        <th>Title</th>
                        <th>Alt</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.photo_id</td>
                            <td><img width="100" height="100" src="@item.photo_url" /></td>
                            <td>@item.photo_title</td>
                            <td>@item.photo_alt</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    }    
}

