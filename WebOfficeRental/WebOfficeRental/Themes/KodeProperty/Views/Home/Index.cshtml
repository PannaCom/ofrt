@{
    ViewBag.Title = "Cho thuê văn phòng chọn gói bất động sản";
}

@{
    string url = Request.Url.Authority;
    if (Request.ServerVariables["HTTPS"] == "on")
    {
        url = "https://" + url;
    }
    else
    {
        url = "http://" + url;
    }
}

